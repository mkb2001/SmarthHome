<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity4">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        android:orientation="vertical"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/textView11"
            android:layout_width="362dp"
            android:layout_height="45dp"
            android:layout_marginLeft="20sp"
            android:layout_marginTop="0sp"
            android:text="The Time is Time"
            android:textSize="23sp"
            android:onClick="showTimePicker"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/divider4"
            android:layout_width="280sp"
            android:layout_height="3dp"
            android:layout_marginLeft="0sp"
            android:background="?android:attr/listDivider" />

        <TextView
            android:id="@+id/textView12"
            android:layout_width="362dp"
            android:layout_height="45dp"
            android:layout_marginTop="12sp"
            android:layout_marginBottom="12sp"
            android:layout_marginLeft="20sp"
            android:text="It is sunset at Location"
            android:textSize="23sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView11"
            app:layout_constraintVertical_bias="0.031" />

        <View
            android:id="@+id/divider5"
            android:layout_width="280sp"
            android:layout_height="3dp"
            android:background="?android:attr/listDivider" />

        <TextView
            android:id="@+id/textView13"
            android:layout_width="362dp"
            android:layout_height="45dp"
            android:layout_marginTop="12sp"
            android:layout_marginLeft="20sp"
            android:layout_marginBottom="12sp"
            android:text="It's sunrise at Location"
            android:textSize="23sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView12"
            app:layout_constraintVertical_bias="0.02" />

        <View
            android:id="@+id/divider6"
            android:layout_width="280sp"
            android:layout_height="3dp"
            android:background="?android:attr/listDivider" />

        <TextView
            android:id="@+id/textView14"
            android:layout_width="362dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="12sp"
            android:layout_marginBottom="12sp"
            android:layout_marginLeft="20sp"
            android:text="It's 15 minutes before sunrise at location"
            android:textSize="23sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView13"
            app:layout_constraintVertical_bias="0.044" />

        <View
            android:id="@+id/divider7"
            android:layout_width="280sp"
            android:layout_height="3dp"
            android:background="?android:attr/listDivider" />

        <TextView
            android:id="@+id/textView15"
            android:layout_width="362dp"
            android:layout_height="70dp"
            android:layout_marginLeft="20sp"
            android:layout_marginTop="12sp"
            android:layout_marginBottom="12sp"
            android:text="It's 15 minutes after sunrise at location"
            android:textSize="23sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView14"
            app:layout_constraintVertical_bias="0.049" />

        <View
            android:id="@+id/divider8"
            android:layout_width="280sp"
            android:layout_height="3dp"
            android:background="?android:attr/listDivider" />

        <TextView
            android:id="@+id/textView16"
            android:layout_width="362dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="12sp"
            android:layout_marginLeft="20sp"
            android:layout_marginBottom="12sp"
            android:text="It's 15 minutes before sunset at location"
            android:textSize="23sp"

            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView15"
            app:layout_constraintVertical_bias="0.081" />

        <View
            android:id="@+id/divider9"
            android:layout_width="280sp"
            android:layout_height="3dp"
            android:background="?android:attr/listDivider" />

        <TextView
            android:id="@+id/textView17"
            android:layout_width="362dp"
            android:layout_height="75dp"
            android:layout_marginTop="12sp"
            android:layout_marginLeft="20sp"
            android:layout_marginBottom="12sp"
            android:text="It's 15 minutes before sunset at location"
            android:textSize="23sp"

            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView16"
            app:layout_constraintVertical_bias="0.223" />


    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>